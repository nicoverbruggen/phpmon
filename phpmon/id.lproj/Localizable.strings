"startup.version_mismatch.desc" = "PHP Monitor melacak versi PHP yang terhubung secara global. Versi global dapat berubah akibat program lain atau Homebrew mungkin sudah mengaitkan formula berbeda setelah pembaruan.";
"startup.version_mismatch.button_switch_back" = "Kembali ke PHP %@";
"startup.version_mismatch.button_stay" = "Tetap gunakan PHP %@";
"startup.unsupported_versions_explanation.title" = "Instalasi PHP tidak didukung oleh Valet terdeteksi!";
